{
  "name": "wp-aggregator-ai-chatbot",
  "version": "1.0.0",
  "description": "A web chat bot using Google Gemini and MCP server",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:safe": "npm run test && node scripts/dev.js",
    "start": "node scripts/start.js",
    "dev:client": "vite",
    "dev:unsafe": "concurrently \"npm run server\" \"vite\"",
    "dev:legacy": "npm run test && concurrently \"npm run server\" \"vite\"",
    "server": "tsx watch server/index.ts",
    "build": "npm run test && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "scrape": "tsx server/scraper/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "cli": "tsx tools/cli/index.ts",
    "build:cli": "tsup tools/cli/index.ts --format esm,cjs --dts --clean --out-dir dist --minify"
  },
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.294.0",
    "mongoose": "^8.0.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "chalk": "^5.5.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "execa": "^9.6.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.1",
    "ora": "^8.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsup": "^8.5.0",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "zod": "^4.0.17"
  },
  "bin": {
    "wpai": "dist/index.cjs"
  }
}
